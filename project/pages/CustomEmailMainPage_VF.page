<apex:page controller="CustomLookupMainPage" sidebar="false" tabStyle="contact" id="page" >
   
 <script>
        var newWin=null;
        function openLookupPopup(name, id)
        {
            //alert('name is ' + name);
           // var strname = 't';
            var strname = document.getElementById('{!$Component.page:form:pgBlock:pbs:targetname}').value;
            //var objName = 'Contact';
            //var fieldname ='Phone';
            //console.log(strname+"****");
            alert('***'+strname);
            var url="/apex/CustomEmailLookup_VF?namefield=" + name + "&idfield=" + id + "&str=" + strname  ;
            newWin=window.open(url, 'Popup','height=500,width=600,left=100,top=100,resizable=no,scrollbars=yes,toolbar=no,status=no');
            if (window.focus) 
            {
                newWin.focus();
            }
                
            return false;
        }
                      
     function closeLookupPopup()
        {
           if (null!=newWin)
           {
              newWin.close();
           }  
        }
    </script>
    <apex:form id="form">
        <apex:pageblock id="pgBlock">
            <apex:pageblockSection id="pbs">
                <apex:outputLabel value="Email">&nbsp; &nbsp; &nbsp;
                    <apex:outputPanel >
                    <apex:inputHidden value="{!accountId}" id="targetId" /> 
                    <apex:inputText value="{!Name}" id="targetname"/>  &nbsp;
                        <apex:commandLink onclick="openLookupPopup('{!$Component.page:form:pgBlock:pbs:targetname}', '{!$Component.targetId}'); return false">
                            <apex:image id="theImage" value="{!$Resource.myResourceImage}" width="15" height="15"/>
                        </apex:commandLink>
                        </apex:outputPanel>
                </apex:outputLabel>        
            </apex:pageblockSection>
        </apex:pageblock>
    </apex:form>
   
</apex:page>